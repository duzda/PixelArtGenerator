; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "ImageTexture",
"class": "GeneratedTexture",
"language": "GDScript",
"path": "res://addons/pixel_art_generator/src/generated_texture.gd"
} ]
_global_script_class_icons={
"GeneratedTexture": ""
}

[application]

config/name="Pixel Art Generator"
run/main_scene="res://demo/demo.tscn"
config/icon="res://icon.png"

[editor_plugins]

enabled=PoolStringArray( "res://addons/pixel_art_generator/plugin.cfg" )

[physics]

common/enable_pause_aware_picking=true
